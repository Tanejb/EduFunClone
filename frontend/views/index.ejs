    <!-- Spinner Start -->
    <div
      id="spinner"
      class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center"
    >
      <div class="spinner-border text-primary" style="width: 3rem; height: 3rem" role="status">
        <span class="sr-only">Loading...</span>
      </div>
    </div>
    <!-- Spinner End -->

    <!-- Navbar Start -->
    <nav class="navbar navbar-expand-lg bg-white navbar-light shadow sticky-top p-0">
      <a href="/" class="navbar-brand d-flex align-items-center px-4 px-lg-5">
        <h2 class="m-0 text-primary"><i class="fa fa-book me-3"></i>EduFun</h2>
      </a>
      <button
        type="button"
        class="navbar-toggler me-4"
        data-bs-toggle="collapse"
        data-bs-target="#navbarCollapse"
      >
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarCollapse">
        <div class="navbar-nav ms-auto p-4 p-lg-0">
          <a href="/" class="nav-item nav-link active">Home</a>
          <a href="/about" class="nav-item nav-link">About</a>
          <a href="/categories" class="nav-item nav-link">Quizzes</a>
          <a href="/chatroom" class="nav-item nav-link">Chatroom</a>
          <div class="nav-item dropdown">
            <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Pages</a>
            <div class="dropdown-menu fade-down m-0">
              <a href="/team" class="dropdown-item">Our Team</a>
              <a href="/testimonial" class="dropdown-item">Testimonial</a>
              <a href="/404" class="dropdown-item">404 Page</a>
            </div>
          </div>
          <a href="/contact" class="nav-item nav-link">Contact</a>
          <% if (session && session.user) { %>
          <a href="/profile/<%= session.user.uid %>" class="nav-item nav-link">Profile</a>
          <% } %>
        </div>
        <% if (session && session.user) { %>
        <!-- If user is signed in, show logout button -->
        <form action="/logout" method="post">
          <button type="submit" class="btn btn-primary py-4 px-lg-5 d-none d-lg-block">
            Log out<i class="fa fa-arrow-right ms-3"></i>
          </button>
        </form>
        <% } else { %>
        <!-- If user is not signed in, show the join now button -->
        <a href="/register" class="btn btn-primary py-4 px-lg-5 d-none d-lg-block">
          Join Now<i class="fa fa-arrow-right ms-3"></i>
        </a>
        <% } %>
      </div>
    </nav>
    <!-- Navbar End -->

    <!-- Carousel Start -->
    <div class="container-fluid p-0 mb-5">
      <div class="owl-carousel header-carousel position-relative">
        <div class="owl-carousel-item position-relative">
          <img class="img-fluid" src="img/carousel-1.jpg" alt="" />
          <div
            class="position-absolute top-0 start-0 w-100 h-100 d-flex align-items-center"
            style="background: rgba(24, 29, 56, 0.7)"
          >
            <div class="container">
              <div class="row justify-content-start">
                <div class="col-sm-10 col-lg-8">
                  <h5 class="text-primary text-uppercase mb-3 animated slideInDown">
                    Best Online Courses
                  </h5>
                  <h1 class="display-3 text-white animated slideInDown">
                    The Best Online Learning Platform
                  </h1>
                  <p class="fs-5 text-white mb-4 pb-2">
                    Welcome to Edu Fun, where learning meets fun! Our platform offers a diverse
                    range of quizzes designed to help you expand your knowledge across various
                    subjects. Whether you're a student preparing for exams, a professional seeking
                    to upskill, or just someone with a passion for learning, Edu Fun provides an
                    engaging and interactive way to enhance your understanding. Start your journey
                    with us today and discover how enjoyable learning can be!
                  </p>
                  <a href="/categories" class="btn btn-primary py-md-3 px-md-5 me-3 animated slideInLeft"
                    >Learn More</a
                  >
                  <% if (session && session.user) { %>
                  <!-- If user is signed in, show logout button -->
                  <form action="/logout" method="post">
                    <button
                      type="submit"
                      class="btn btn-light py-md-3 px-md-5 animated slideInRight"
                    >
                      Log out
                    </button>
                  </form>
                  <% } else { %>
                  <!-- If user is not signed in, show the join now button -->
                  <a href="/register" class="btn btn-light py-md-3 px-md-5 animated slideInRight">
                    Join Now
                  </a>
                  <% } %>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="owl-carousel-item position-relative">
          <img class="img-fluid" src="img/carousel-2.jpg" alt="" />
          <div
            class="position-absolute top-0 start-0 w-100 h-100 d-flex align-items-center"
            style="background: rgba(24, 29, 56, 0.7)"
